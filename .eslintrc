{
    "env": {
        "node": true
    },
    "extends": "airbnb-base",
    "rules": {
        "no-process-exit": 0,
        "no-use-before-define": 0,
        "no-console": 0,
        "indent": 0,
        "func-names": 0,
        "consistent-return": 0,
        "no-param-reassign": 0,
        "quotes": 0
    }
}
